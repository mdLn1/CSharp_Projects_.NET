@model dynamic

@{
    ViewData["Title"] = "Home Page";
}

<h1>Dynamic Model</h1>

<div class="container">
    <div class="form-group">
        <label class="form-control">
            Student name:
        </label>
        @*Inside the value attribute of the input field you can see we used @ViewData["studentName"] to display its value*@
        <input value="@ViewData["studentName"]" class="form-control" style="border:5px solid blue;"/>
    </div>
    <div class="form-group">
        <label class="form-control">
            Student Date of Birth:
        </label>
        @*Inside the value attribute of the input field you can see we used @ViewBag.studentDateOfBirth to display its value*@
        <input value="@ViewBag.studentDateOfBirth" class="form-control" style="border:5px solid blue;" />
    </div>
    <div class="form-group">
        <label class="form-control">
            Student Age:
        </label>
        @*Inside the value attribute of the input field you can see we used @Model.Age to display its value*@
        <input value="@Model.Age" class="form-control" style="border:5px solid blue;" />
    </div>
    <div class="form-group">
        <label class="form-control">
            Student Programme of Study:
        </label>
        @*Inside the value attribute of the input field you can see we used @Model.ProrgammeOfStudy to display its value*@
        <input value="@Model.ProgrammeOfStudy" class="form-control"  style="border:5px solid blue;"/>
    </div>
</div>
